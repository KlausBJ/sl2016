<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @ticket_type.name %>
</p>

<table>
	<tr>
		<th>Tilmelding</th>
	</tr>
	<% @ticket_type.registrations.order('name').each do |registration| %>
		<tr>
			<td><%= registration.name %> - <%= registration.id %></td>
		</tr>
	<% end %>
</table>

<%= link_to 'Edit', edit_ticket_type_path(@ticket_type) %> |
<%= link_to 'Back', ticket_types_path %>
